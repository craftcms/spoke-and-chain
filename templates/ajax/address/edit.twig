{% set id = craft.app.request.getQueryParam('id', null) %}
<!-- Template: {{ _self }}.twig -->
<div class="ajax-output">
  {% if not id %}
    <p>{{ 'Uanble to find address.'|t }}</p>
  {% endif %}

  {% set address = id == 'new' ? [] : craft.commerce.addresses.getAddressByIdAndCustomerId(id, craft.commerce.customers.customer.id) %}
  {% if not address and id != 'new' %}
    <p>{{ 'Uanble to find address.'|t }}</p>
  {% else %}
    {% include '_includes/components/address/fields' with {
      modelName: 'address',
      model: address,
      outputIdField: true,
    } %}
  {% endif %}
</div>